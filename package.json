{
  "name": "react-wittr",
  "version": "1.0.0",
  "description": "A fake social app in react inspired from wittr",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:server": "NODE_ENV=test nodemon --exec babel-node ./server/index.js",
    "dev:client": "webpack --watch",
    "prod:client": "webpack -p",
    "prod:server": "babel server --out-dir dist",
    "prestart": "npm run prod:client && npm run prod:server",
    "start": "NODE_ENV=production node dist/index.js",
    "lint": "eslint ./src/**/*.js"
  },
  "keywords": [
    "react",
    "wittr"
  ],
  "author": "Anoop Mundathan <anoop.mundathan@gmail.com> (http://anoopmundathan.me/)",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.2",
    "eslint": "3.19.0",
    "eslint-plugin-react": "7.0.1",
    "html-webpack-plugin": "2.28.0",
    "node-sass": "4.5.3",
    "sass-loader": "6.0.5",
    "style-loader": "0.18.0",
    "webpack": "2.5.1",
    "webpack-dev-server": "2.4.5"
  },
  "dependencies": {
    "express": "4.15.3",
    "react": "15.5.4",
    "react-dom": "15.5.4"
  }
}
