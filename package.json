{
  "name": "react-wittr",
  "version": "1.0.0",
  "description": "A fake social app in react inspired from wittr",
  "main": "index.js",
  "engines": {
    "node": "v7.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rm -rf build && mkdir ./build",
    "build:cpy": "cp -rf ./public/imgs ./build/imgs & cp -rf ./public/avatars ./build/avatars",
    "build:client": "webpack -p",
    "build": "npm-run-all --parallel build:*",
    "prestart": "npm run build",
    "start": "node server/index.js",
    "predev": "npm run build",
    "dev": "concurrently --kill-others \"nodemon server/index.js\" \"webpack -w\""
  },
  "keywords": [
    "react",
    "wittr"
  ],
  "author": "Anoop Mundathan <anoop.mundathan@gmail.com> (http://anoopmundathan.me/)",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "concurrently": "3.4.0",
    "css-loader": "0.28.2",
    "eslint": "3.19.0",
    "eslint-plugin-react": "7.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "node-sass": "4.5.3",
    "npm-run-all": "4.0.2",
    "sass-loader": "6.0.5",
    "style-loader": "0.18.0",
    "webpack": "2.5.1"
  },
  "dependencies": {
    "express": "4.15.3",
    "react": "15.5.4",
    "react-dom": "15.5.4"
  }
}
